{
  "printWidth": 90,
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "es5",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react",
    "^next",
    "<THIRD_PARTY_MODULES>",

    "^@/api/(.*)$",
    "^@/hooks/(.*)$", 
    "^@/components/(.*)$",
    "^@/app/(.*)$",
    "^@/assets/(.*)$",
    "^@/theme/(.*)$",
    "^@/store/(.*)$",
    "^@/utils/(.*)$",

    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
